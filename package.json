{
  "name": "ang2-webpack-startup",
  "version": "0.0.4",
  "description": "Angular2 Webpack Startup",
  "scripts": {
    "start": "webpack-dev-server --inline --progress --port 8080",
    "start:host": "npm start -- --host 0.0.0.0",
    "test": "karma start",
    "test:single": "karma start --auto-watch false --single-run true",
    "debug:test": "npm test -- --debug true",
    "build": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail",
    "postinstall": "typings install",
    "lint": "tslint --force \"src/**/*.ts\"",
    "showcoverage": "lite-server -c config/coveragebrowser.json"
  },
  "dependencies": {
    "@angular/common": "2.1.0",
    "@angular/compiler": "2.1.0",
    "@angular/core": "2.1.0",
    "@angular/http": "2.1.0",
    "@angular/platform-browser": "2.1.0",
    "@angular/platform-browser-dynamic": "2.1.0",
    "@angular/router": "3.1.0",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "jquery": "1.9.1 - 2",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.0-rc.1",
    "zone.js": "0.6.25"
  },
  "license": "MIT",
  "devDependencies": {
    "angular2-template-loader": "^0.5.0",
    "codelyzer": "0.0.25",
    "css-loader": "^0.23.1",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.15.0",
    "istanbul": "^0.4.4",
    "istanbul-instrumenter-loader": "^0.2.0",
    "jasmine-core": "^2.4.1",
    "jasmine-spec-reporter": "^2.5.0",
    "karma": "^1.2.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.0.4",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-remap-istanbul": "^0.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "lite-server": "^2.2.2",
    "node-sass": "^3.8.0",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.7",
    "postcss-loader": "^0.9.1",
    "raw-loader": "^0.5.1",
    "remap-istanbul": "^0.6.4",
    "resolve-url-loader": "^1.6.0",
    "rimraf": "^2.5.2",
    "sass-loader": "^4.0.0",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.8.1",
    "tslint": "^3.13.0",
    "tslint-loader": "^2.1.5",
    "typescript": "^2.0.3",
    "typings": "^1.3.2",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.14.0"
  },
  "jshintConfig": {
    "esversion": 6
  }
}
